                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-12">
                            <div class="item-address-part">
                                <div class="icon-part">
                                    <i class="flaticon-location"></i>
                                </div>
                                <div class="address-text">
                                    <h3 class="contact-title">Estamos Ubicados</h3>
                                    <span>Jorge Rodríguez y Amazonas.</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div style="width:70%; margin:0 auto;">
                    <div class="sec-title mb-0">
                        <h2 class="title pb-15">DÉJANOS UN MENSAJE</h2>
                    </div>
                    
                    <script charset="utf-8" type="text/javascript" src="//js.hsforms.net/forms/embed/v2.js"></script>

                    <script>

                        hbspt.forms.create({

                            region: "na1",

                            portalId: "8488119",

                            formId: "cecf9ef4-7172-4540-80df-d1080a9a75d6"

                        });

                    </script>
                </div>
               @* <div class="sec-title mb-50 text-center">
                    <iframe src="https://snazzymaps.com/embed/264662" width="100%" height="600px" style="border:none;"></iframe>
                </div>*@
